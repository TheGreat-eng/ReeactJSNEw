# Production Environment Variables
VITE_BASE_URL=https://your-production-api.com
VITE_API_BASE_URL=https://your-production-api.com/api/v1
VITE_APP_NAME=React User Management
VITE_APP_VERSION=1.0.0
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# Image/Asset URLs
VITE_AVATAR_BASE_URL=https://your-production-api.com/images/avatar
VITE_UPLOAD_BASE_URL=https://your-production-api.com/uploads

# Authentication
VITE_TOKEN_STORAGE_KEY=auth_token
VITE_REFRESH_TOKEN_KEY=refresh_token

# API Configuration
VITE_API_TIMEOUT=15000
VITE_MAX_FILE_SIZE=5242880

# Security
VITE_SECURE_COOKIES=true
VITE_HTTPS_ONLY=true